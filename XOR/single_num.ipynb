   "source": [
    "class Solution:\n",
    "  def findSingleNumber(self, arr):\n",
    "    x = arr[0]\n",
    "    for i in range(1, len(arr)):\n",
    "        x = x ^ arr[i]\n",
    "    return x\n",
    "sol = Solution()"
   ]
